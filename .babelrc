{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 3 versions",
            "ie >= 9",
            "ios >= 8",
            "android >= 4.4"
          ]
        },
        "useBuiltIns": "usage"
      }
    ],
    "stage-0"
  ],
  "plugins": [
    [
      "component",
      [
        {
          "libraryName": "mint-ui",
          "style": true
        }
      ]
    ],
    "transform-runtime"
  ]
}
